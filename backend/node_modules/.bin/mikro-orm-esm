#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/abomb/Desktop/DiscGolf-Tracker/backend/node_modules/.pnpm/@mikro-orm+cli@5.7.2_@mikro-orm+migrations@5.7.2_@mikro-orm+postgresql@5.7.2_@mikro-orm+seeder@5.7.2_pg@8.10.0/node_modules/@mikro-orm/cli/node_modules:/Users/abomb/Desktop/DiscGolf-Tracker/backend/node_modules/.pnpm/@mikro-orm+cli@5.7.2_@mikro-orm+migrations@5.7.2_@mikro-orm+postgresql@5.7.2_@mikro-orm+seeder@5.7.2_pg@8.10.0/node_modules/@mikro-orm/node_modules:/Users/abomb/Desktop/DiscGolf-Tracker/backend/node_modules/.pnpm/@mikro-orm+cli@5.7.2_@mikro-orm+migrations@5.7.2_@mikro-orm+postgresql@5.7.2_@mikro-orm+seeder@5.7.2_pg@8.10.0/node_modules:/Users/abomb/Desktop/DiscGolf-Tracker/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/abomb/Desktop/DiscGolf-Tracker/backend/node_modules/.pnpm/@mikro-orm+cli@5.7.2_@mikro-orm+migrations@5.7.2_@mikro-orm+postgresql@5.7.2_@mikro-orm+seeder@5.7.2_pg@8.10.0/node_modules/@mikro-orm/cli/node_modules:/Users/abomb/Desktop/DiscGolf-Tracker/backend/node_modules/.pnpm/@mikro-orm+cli@5.7.2_@mikro-orm+migrations@5.7.2_@mikro-orm+postgresql@5.7.2_@mikro-orm+seeder@5.7.2_pg@8.10.0/node_modules/@mikro-orm/node_modules:/Users/abomb/Desktop/DiscGolf-Tracker/backend/node_modules/.pnpm/@mikro-orm+cli@5.7.2_@mikro-orm+migrations@5.7.2_@mikro-orm+postgresql@5.7.2_@mikro-orm+seeder@5.7.2_pg@8.10.0/node_modules:/Users/abomb/Desktop/DiscGolf-Tracker/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --loader ts-node/esm --no-warnings "$basedir/../@mikro-orm/cli/esm.js" "$@"
else
  exec node  --loader ts-node/esm --no-warnings "$basedir/../@mikro-orm/cli/esm.js" "$@"
fi
